<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="WorkflowService" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://www.w3schools.com" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" targetNamespace="http://www.w3schools.com">
<wsdl:import location="WorkflowSearchingService.wsdl" namespace="http://www.w3schools.com"></wsdl:import>
<wsdl:types>
	<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.w3schools.com">
		<xsd:element name="processID" type="xsd:string"></xsd:element>
		<xsd:element name="out" type="xsd:string"></xsd:element>
		<xsd:element name="takeActionType">
			<xsd:complexType>
				<xsd:sequence>
					<xsd:element name="name" type="xsd:string"/>
					<xsd:element name="role" type="xsd:string"/>
					<xsd:element name="actionId" type="xsd:string"/>
				</xsd:sequence>
			</xsd:complexType>
		</xsd:element>
		<xsd:element name="nextActionType">
			<xsd:complexType >
				<xsd:sequence>
					<xsd:element ref="actionName" />
				</xsd:sequence>
			</xsd:complexType>
		</xsd:element>
	</xsd:schema>
    </wsdl:types>
    <wsdl:message name="createProcessRequest">
    	<wsdl:part name="workflowName" element="tns:workflowName"></wsdl:part>
    </wsdl:message>
    <wsdl:message name="createProcessResponse">
    	<wsdl:part name="processID" element="tns:processID"></wsdl:part>
    </wsdl:message>
    <wsdl:message name="systemFault">
    	<wsdl:part name="fault" element="tns:fault"></wsdl:part>
    </wsdl:message>
    <wsdl:message name="getProcessRequest">
    	<wsdl:part name="processID" element="tns:processID"></wsdl:part>
    </wsdl:message>
    <wsdl:message name="getProcessResponse">
    	<wsdl:part name="processInfo" element="tns:process"></wsdl:part>
    </wsdl:message>
    <wsdl:message name="unknownProcessFault">
    	<wsdl:part name="fault" element="tns:fault"></wsdl:part>
    </wsdl:message>
    <wsdl:message name="takeActionRequest">
    	<wsdl:part name="params" element="tns:takeActionType"></wsdl:part>
    </wsdl:message>
    <wsdl:message name="takeActionResponse">
    	<wsdl:part name="out" element="tns:out"></wsdl:part>
    </wsdl:message>
    <wsdl:message name="AlreadyTakenFault">
    	<wsdl:part name="fault" element="tns:fault"></wsdl:part>
    </wsdl:message>
    <wsdl:message name="DifferentRoleFault">
    	<wsdl:part name="fault" element="tns:fault"></wsdl:part>
    </wsdl:message><!--
    <wsdl:message name="completeActionRequest">
    	<wsdl:part name="nextAction" element="tns:nextActionType"></wsdl:part>
    </wsdl:message>
    <wsdl:message name="completeActionResponse">
    	<wsdl:part name="out" element="tns:out"></wsdl:part>
    </wsdl:message>-->
    <wsdl:portType name="JoinWorkflow">
    	<wsdl:operation name="createProcess">
    		<wsdl:input message="tns:createProcessRequest"></wsdl:input>
    		<wsdl:output message="tns:createProcessResponse"></wsdl:output>
            <wsdl:fault name="systemFault" message="tns:systemFault"></wsdl:fault>
        </wsdl:operation>
    	<wsdl:operation name="getProcess">
    		<wsdl:input message="tns:getProcessRequest"></wsdl:input>
    		<wsdl:output message="tns:getProcessResponse"></wsdl:output>
            <wsdl:fault name="unknownProcessFault" message="tns:unknownProcessFault"></wsdl:fault>
        </wsdl:operation>
    	<wsdl:operation name="takeAction">
    		<wsdl:input message="tns:takeActionRequest"></wsdl:input>
    		<wsdl:output message="tns:takeActionResponse"></wsdl:output>
            <wsdl:fault name="AlreadyTakenFault" message="tns:AlreadyTakenFault"></wsdl:fault>
            <wsdl:fault name="DifferentRoleFault" message="tns:DifferentRoleFault"></wsdl:fault>
        </wsdl:operation><!--
    	<wsdl:operation name="completeAction">
    		<wsdl:input message="tns:completeActionRequest"></wsdl:input>
    		<wsdl:output message="tns:completeActionResponse"></wsdl:output>
        </wsdl:operation>-->
    </wsdl:portType>
    <wsdl:binding name="WorkflowImplPortBinding" type="tns:JoinWorkflow">
    	<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />
    	<wsdl:operation name="createProcess">
    		<soap:operation	soapAction="" />
    		<wsdl:input>
    			<soap:body use="literal" />
    		</wsdl:input>
    		<wsdl:output>
    			<soap:body use="literal" />
    		</wsdl:output>
    		<wsdl:fault name="systemFault">
    			<soap:fault use="literal" name="systemFault" />
    		</wsdl:fault>
    	</wsdl:operation>
    	<wsdl:operation name="getProcess">
    		<soap:operation	soapAction="" />
    		<wsdl:input>
    			<soap:body use="literal" />
    		</wsdl:input>
    		<wsdl:output>
    			<soap:body use="literal" />
    		</wsdl:output>
    		<wsdl:fault name="unknownProcessFault">
    			<soap:fault use="literal" name="unknownProcessFault" />
    		</wsdl:fault>
    	</wsdl:operation>
    	<wsdl:operation name="takeAction">
    		<soap:operation	soapAction="" />
    		<wsdl:input>
    			<soap:body use="literal" />
    		</wsdl:input>
    		<wsdl:output>
    			<soap:body use="literal" />
    		</wsdl:output>
    		<wsdl:fault name="AlreadyTakenFault">
    			<soap:fault use="literal" name="AlreadyTakenFault" />
    		</wsdl:fault>
    		<wsdl:fault name="DifferentRoleFault">
    			<soap:fault use="literal" name="DifferentRoleFault" />
    		</wsdl:fault>
    	</wsdl:operation><!--
    	<wsdl:operation name="completeAction">
    		<soap:operation	soapAction="" />
    		<wsdl:input>
    			<soap:body use="literal" />
    		</wsdl:input>
    		<wsdl:output>
    			<soap:body use="literal" />
    		</wsdl:output>
    	</wsdl:operation>-->
    </wsdl:binding>
    <wsdl:service name="WorkflowService">
    	<wsdl:port name="WorkflowPort" binding="WorkflowImplPortBinding">
    		<soap:address location="http://localhost:7070/wfcontrol" />
    	</wsdl:port>
    </wsdl:service>
</wsdl:definitions>
