<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<definitions name="WorkflowSearchingService" targetNamespace="http://pad.polito.it/WorkflowSearching" xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:wsp="http://www.w3.org/ns/ws-policy" xmlns:tns="http://pad.polito.it/WorkflowSearching" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:wsp1_2="http://schemas.xmlsoap.org/ws/2004/09/policy" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsam="http://www.w3.org/2007/05/addressing/metadata" xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd">
  <types>
    <xsd:schema>
      <xsd:import namespace="http://pad.polito.it/WorkflowSearching" schemaLocation="WFInfo.xsd"/>
    </xsd:schema>
	<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://pad.polito.it/WorkflowSearching">
		<xsd:element name="workflowName" type="xsd:string"></xsd:element>
		<xsd:element name="fault" type="xsd:string"></xsd:element>
		<xsd:element name="getWorkflowsType">
			<xsd:complexType>
				<xsd:sequence />
			</xsd:complexType>
		</xsd:element>
		<xsd:element name="getProcessesType">
			<xsd:complexType>
				<xsd:sequence>
					<xsd:element name="process" type="tns:processType" maxOccurs="unbounded" minOccurs="0"></xsd:element>
				</xsd:sequence>
			</xsd:complexType>
		</xsd:element>
	</xsd:schema>
  </types>
  <message name="getWorkflowsRequest">
    <part name="emptyParam" element="tns:getWorkflowsType"/>
  </message>
  <message name="getWorkflowsResponse">
    <part name="listOfWorkflow" element="tns:wfInfo"/>
  </message>
  <message name="getProcessesRequest">
    <part name="workflowName" element="tns:workflowName"/>
  </message>
  <message name="getProcessesResponse">
    <part name="listOfProcess" element="tns:getProcessesType"/>
  </message>
  <message name="UnknownWorkflow">
    <part name="fault" element="tns:fault"/>
  </message>
  <portType name="WorkflowSearching">
    <operation name="getWorkflows">
      <input message="tns:getWorkflowsRequest"/>
      <output message="tns:getWorkflowsResponse"/>
    </operation>
    <operation name="getProcesses">
      <input message="tns:getProcessesRequest"/>
      <output message="tns:getProcessesResponse"/>
      <fault message="tns:UnknownWorkflow" name="UnknownWorkflow"/>
    </operation>
  </portType>
  <binding name="WorkflowSearchingImplPortBinding" type="tns:WorkflowSearching">
    <soap:binding transport="http://schemas.xmlsoap.org/soap/http" style="document"/>
    <operation name="getWorkflows">
      <soap:operation soapAction=""/>
      <input>
        <soap:body use="literal"/>
      </input>
      <output>
        <soap:body use="literal"/>
      </output>
    </operation>
    <operation name="getProcesses">
      <soap:operation soapAction=""/>
      <input>
        <soap:body use="literal"/>
      </input>
      <output>
        <soap:body use="literal"/>
      </output>
      <fault name="UnknownWorkflow">
        <soap:fault name="UnknownWorkflow" use="literal"/>
      </fault>
    </operation>
  </binding>
  <service name="WorkflowSearchingService">
    <port name="WorkflowSearchingImplPort" binding="tns:WorkflowSearchingImplPortBinding">
      <soap:address location="http://localhost:7071/wfinfo"/>
    </port>
  </service>
</definitions>
