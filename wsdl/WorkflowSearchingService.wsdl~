<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wsdl:definitions name="WorkflowSearchingService" xmlns:tns="http://www.w3schools.com" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:wsp="http://www.w3.org/ns/ws-policy" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:wsp1_2="http://schemas.xmlsoap.org/ws/2004/09/policy" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsam="http://www.w3.org/2007/05/addressing/metadata" xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" targetNamespace="http://www.w3schools.com">
  <wsdl:types>
    <xsd:schema>
      <xsd:import namespace="http://www.w3schools.com" schemaLocation="WFInfo.xsd"/>
    </xsd:schema>
	<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.w3schools.com">
		<xsd:element name="workflowName" type="xsd:string"></xsd:element>
		<xsd:element name="fault" type="xsd:string"></xsd:element>
		<xsd:element name="getWorkflowsType">
			<xsd:complexType>
				<xsd:sequence />
			</xsd:complexType>
		</xsd:element>
		<xsd:element name="getProcessesType">
			<xsd:complexType>
				<xsd:sequence>
					<xsd:element name="process" type="processType" maxOccurs="unbounded" minOccurs="0"></xsd:element>
				</xsd:sequence>
			</xsd:complexType>
		</xsd:element>
	</xsd:schema>
  </wsdl:types>
  <wsdl:message name="getWorkflowsRequest">
    <wsdl:part name="emptyParam" element="tns:getWorkflowsType"/>
  </wsdl:message>
  <wsdl:message name="getWorkflowsResponse">
    <wsdl:part name="listOfWorkflow" element="tns:wfInfo"/>
  </wsdl:message>
  <wsdl:message name="getProcessesRequest">
    <wsdl:part name="workflowName" element="tns:workflowName"/>
  </wsdl:message>
  <wsdl:message name="getProcessesResponse">
    <wsdl:part name="listOfProcess" element="tns:getProcessesType"/>
  </wsdl:message>
  <wsdl:message name="UnknownWorkflow">
    <wsdl:part name="fault" element="tns:fault"/>
  </wsdl:message>
  <wsdl:portType name="WorkflowSearching">
    <wsdl:operation name="getWorkflows">
      <wsdl:input message="tns:getWorkflowsRequest"/>
      <wsdl:output message="tns:getWorkflowsResponse"/>
    </wsdl:operation>
    <wsdl:operation name="getProcesses">
      <wsdl:input message="tns:getProcessesRequest"/>
      <wsdl:output message="tns:getProcessesResponse"/>
      <wsdl:fault message="UnknownWorkflow" name="UnknownWorkflow"/>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name="WorkflowSearchingImplPortBinding" type="tns:WorkflowSearching">
    <soap:binding transport="http://schemas.xmlsoap.org/soap/http" style="document"/>
    <wsdl:operation name="getWorkflows">
      <soap:operation soapAction=""/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="getProcesses">
      <soap:operation soapAction=""/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
      <wsdl:fault name="UnknownWorkflow">
        <soap:fault name="UnknownWorkflow" use="literal"/>
      </wsdl:fault>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="WorkflowSearchingService">
    <wsdl:port name="WorkflowSearchingImplPort" binding="WorkflowSearchingImplPortBinding">
      <soap:address location="http://localhost:7071/wfinfo"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
